generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Entry {
  id        Int      @id @default(autoincrement())
  text      String
  calories  Int
  protein   Int
  fat       Int
  carbs     Int
  createdAt DateTime @default(now())
}
